{
  "name": "@zoralabs/1155-deployments",
  "repository": "https://github.com/ourzora/zora-protocol",
  "license": "MIT",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "FOUNDRY_PROFILE=dev forge test --watch -vvv",
    "test": "pnpm generate && forge test -vvv",
    "copy-deployed-contracts": "pnpm tsx script/copy-deployed-contracts.ts",
    "unlink-common": "rm -rf ./node_modules/@zoralabs/shared-contracts && cp -r ../shared-contracts ./node_modules/@zoralabs/shared-contracts",
    "unlink-protocol-rewards": "rm -rf ./node_modules/@zoralabs/protocol-rewards && cp -r ../protocol-rewards ./node_modules/@zoralabs/protocol-rewards",
    "unlink-1155": "rm -rf ./node_modules/@zoralabs/zora-1155-contracts && cp -r ../1155-contracts ./node_modules/@zoralabs/zora-1155-contracts",
    "unlink-contracts": "pnpm unlink-common && pnpm run unlink-protocol-rewards && pnpm run unlink-1155",
    "link-contracts": "rm -rf ./node_modules && cd .. && pnpm install",
    "update-pending-upgrades": "pnpm tsx script/getPendingUpgrades.ts",
    "generate": "pnpm tsx script/generateForkTests.ts",
    "setup:implementations": "source .env && forge script script/DeployMintersAndImplementations.s.sol --rpc-url https://sepolia.base.org --broadcast --verify --etherscan-api-key $ETHERSCAN_API_KEY --private-key $PRIVATE_KEY",
    "setup:copy-addresses": "source .env && pnpm tsx script/copy-deployed-contracts.ts",
    "setup:factory-proxy": "source .env && forge script script/DeployFactoryProxy.s.sol --rpc-url https://sepolia.base.org --broadcast --verify --etherscan-api-key $ETHERSCAN_API_KEY --private-key $PRIVATE_KEY",
    "deploy-all": "pnpm setup:implementations && pnpm setup:copy-addresses && pnpm setup:factory-proxy"
  },
  "devDependencies": {
    "@lavamoat/preinstall-always-fail": "2.0.0",
    "@openzeppelin/contracts": "4.9.2",
    "@turnkey/api-key-stamper": "^0.3.1",
    "@turnkey/http": "^2.5.1",
    "@turnkey/viem": "^0.4.4",
    "@types/node": "^20.1.2",
    "@zoralabs/openzeppelin-contracts-upgradeable": "4.8.4",
    "@zoralabs/protocol-rewards": "workspace:^",
    "@zoralabs/shared-contracts": "workspace:^",
    "@zoralabs/sparks-contracts": "workspace:^",
    "@zoralabs/tsconfig": "workspace:^",
    "@zoralabs/zora-1155-contracts": "workspace:^",
    "ds-test": "https://github.com/dapphub/ds-test#cd98eff28324bfac652e63a239a60632a761790b",
    "es-main": "^1.2.0",
    "forge-std": "https://github.com/foundry-rs/forge-std#705263c95892a906d7af65f0f73ce8a4a0c80b80",
    "glob": "^10.2.2",
    "graphql-request": "^6.1.0",
    "handlebars": "^4.7.8",
    "semver": "^7.5.8",
    "solady": "0.0.132",
    "solmate": "6.1.0",
    "toml": "^3.0.0",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "@zoralabs/chains": "^1.4.1",
    "viem": "^2.9.19"
  }
}
